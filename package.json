{
  "name": "wolfbot",
  "version": "3.0.0",
  "description": "Advanced Facebook Messenger Bot with command system, events, and rental management",
  "main": "wolfbot.js",
  "type": "commonjs",
  "homepage": "https://github.com/ngdgnam/WOLFBOT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngdgnam/WOLFBOT.git"
  },
  "bugs": {
