  },
  "author": "ngdgnam",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "node wolfbot.js",
    "dev": "NODE_ENV=development node wolfbot.js",
    "setup": "bash setup.sh auto",
    "setup:termux": "bash setup.sh termux",
    "setup:vps": "bash setup.sh vps",
    "install:termux": "bash install-termux.sh",
    "install:vps": "bash install-vps.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stop": "pkill -f 'node wolfbot.js'",
    "restart": "npm stop && sleep 2 && npm start"
  },
  "keywords": [
    "facebook",
    "messenger",
    "bot",
    "fca",
    "command",
    "event",
    "nodejs"
  ],
